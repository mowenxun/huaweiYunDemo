            #{item.createdTime}, #{item.updatedTime}, #{item.updatedBy}, #{item.shopOrderId}, #{item.orderCode},
            #{item.unitId}, #{item.unitName}, #{item.skuId}, #{item.shopId}, #{item.shopName}, #{item.shopCode},
