        </if>
        <if test="shopOrderIds != null and shopOrderIds.size() > 0">
            and shop_order_id in
            <foreach item="item" index="index" collection="shopOrderIds" open="(" separator="," close=")">
                #{item}
            </foreach>
        </if>

        <if test="skuNameLike != null and skuNameLike != ''">
            and sku_name like concat('%',trim(#{skuNameLike}),'%')
        </if>
        <if test="skuCodeLike != null and skuCodeLike != ''">
            and lower(sku_code) like concat('%',lower(trim(#{skuCodeLike})),'%')
        </if>
